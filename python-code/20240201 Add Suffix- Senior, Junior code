def add_suffix(web_str_list, suffix):
    new_list = [web_str + suffix for web_str in web_str_list]   # senior code
    """
    # junior code
    new_list = []
    for web_str in web_str_list:
        new_list.append(web_str + suffix)
    """
    return new_list
   
result_list = add_suffix(["yahoo", "google"], ".com")
print(result_list)
