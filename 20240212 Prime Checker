def prime_checker(num):
    if num <= 1:
        return False
    if num == 2:
        return True
    if num % 2 == 0:
        return False
    for i in range(2, int(num**0.5)+1):
        if num % i == 0:
            return False
    else:
        return True

num = int(input('Enter a numberï¼š'))
print(prime_checker(num))
