def fizzbuzz_list(number):
    num_list = []
    for i in range(1,number + 1):
        if i % 15 == 0:
            num_list.append("FizzBuzz")
        elif i % 5 == 0:
            num_list.append("Buzz")
        elif i % 3 == 0:
            num_list.append("Fizz") 
        else:
            num_list.append(i)
    return num_list

number = 18
result = fizzbuzz_list(number)
print(result)
